{
  "name": "gRPC Web Devtools",
  "version": "1.1",
  "description": "Extend developer tools with a grpc-web debugger",
  "devtools_page": "devtools.html",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "exclude_globs": [ "https://www.google*" ],
      "js": ["inject.js"],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "web_accessible_resources" : [
    "inject.js",
    "pageScript.js"
  ]
}
